<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".test.TestActivity"
    >


    <Button
        android:id="@+id/btngent"
        android:text="통신사/wifi 연결 check"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_api"
        android:text="API테스트"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnupdate"
        android:text="유저미션업데이트"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_webview"
        android:text="웹뷰띠우기"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/progressstart"
        android:text="시작"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/progressstop"
        android:text="종료"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_play_again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/play_again" />

    <ProgressBar
        android:id="@+id/progcircle"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:visibility="invisible"
        />

    <Button
        android:id="@+id/btn_uplaod_user"
        android:text="업로드 테스트"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>